#Qaisya Dwi Aryana 121140063 RB

class Profil:
    def __init__(self, nama, nim, kelas, sks):
        self.nama = nama
        self.nim = nim
        self.kelas = kelas
        self.sks = sks
    
data = Profil("qaisya", 121140063, "RB", 22)
print(data.nama)
print(data.nim)
print(data.kelas)
print(data.sks)
